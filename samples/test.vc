func fibonacci(bolivar number) {
    bolivar n1 = bolivar n2 = 1
    bolivar nth = 0
    bolivar count = 0

    suppose that (number <= 0)
    then {
        print("Number should be more than 0")
        return
    } otherwise that (number == 1)
    then {
        print(n1)
        return
    } other {
        stop (count < number)
        cycle {
            print (n1)
            nth = n1+n2
            n1 = n2+0
            n2 = nth+0
            count = count + 1
        }
    }
}


fibonacci(10)
